<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.VidoeChat"
        tools:targetApi="31">
        <activity
            android:name=".SettingAccount"
            android:exported="false"
            android:label="@string/title_activity_setting_account"
            android:theme="@style/Theme.VidoeChat" />
        <activity
            android:name=".FunctionalMenu"
            android:exported="false"
            android:label="@string/title_activity_functional_menu"
            android:theme="@style/Theme.VidoeChat" />
        <activity
            android:name=".SingUP"
            android:exported="false"
            android:label="@string/title_activity_sing_up"
            android:theme="@style/Theme.VidoeChat" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.VidoeChat">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>